{
	"name": "cryptodira/phpole",
	"type": "library",
	"description": "Native PHP manipulation of Microsoft OLE compound files",
	"license": "GPL-3.0+",
	"keywords": [
		"Microsoft OLE"
	],
	"autoload": {
		"files": [
			"src/OleUtil.php",
			"src/OleConst.php"
		],
